<?xml version="1.0"?>
<extension key="branch" type="module">
  <file>branch</file>
  <name>RiverLea Branch: GPO/Albany</name>
  <description>RiverLea subtheme/stream for Green Party of Ontario, for JMA by NW</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>JMA Consulting</author>
    <author>Nicol Wistreich</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/branch</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-06</releaseDate>
  <version>1.0.0-alpha</version>
  <develStage>Work in Progress</develStage>
  <compatibility>
    <ver>5.80</ver>
  </compatibility>
  <comments>A cross-CMS CiviCRM admin theme framework.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/branch</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>theme-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>setting-admin@1.0.1</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
</extension>
